DEFINE p_source        LIKE type_t.chr10
DEFINE l_fmii RECORD  #外匯公允價值調整帳務主檔
       fmiidocno LIKE fmii_t.fmiidocno, #公允價值調整帳務單號
       fmiicomp  LIKE fmii_t.fmiicomp,  #歸屬法人
       fmiisite  LIKE fmii_t.fmiisite,  #資金中心
       fmiidocdt LIKE fmii_t.fmiidocdt, #單據日期
       fmiild    LIKE fmii_t.fmiild,    #帳套
       fmii001   LIKE fmii_t.fmii001,   #年度
       fmii002   LIKE fmii_t.fmii002,   #期別

         LET g_errparam.replace[1] = 'aooi100'
         LET g_errparam.replace[2] = cl_get_progname('aooi100',g_lang,"2")
         LET g_errparam.exeprog = 'aooi100'
         #160321-00016#19 --e add
         LET g_errparam.code = g_errno
         LET g_errparam.popup = TRUE
         CALL cl_err()
      END IF 
   ELSE

   #======================================  
   #該科目做部門管理時，部門不可空白  
   IF l_glad007 = 'Y' THEN
      IF cl_null(g_glam.glam020) THEN
         LET r_success = FALSE
#         CALL cl_errmsg('glam020',l_errmsg,g_glam.glam020,'agl-00043',1)

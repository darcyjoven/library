      LET g_errparam.code = SQLCA.sqlcode
      LET g_errparam.extend = 'upd glap_t'
      LET g_errparam.popup = TRUE
      CALL cl_err()

      RETURN r_success
   END IF
   
   LET r_success = TRUE
   RETURN r_success

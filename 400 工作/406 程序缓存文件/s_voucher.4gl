         CALL cl_err()
      ELSE
         #客商检查 
         CALL s_voucher_glaq021_chk(g_glam.glam023)
         IF NOT cl_null(g_errno) THEN
            LET r_success = FALSE
#            CALL cl_errmsg('glam023',l_errmsg,g_glam.glam023,g_errno,1)

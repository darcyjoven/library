       IF l_glad026 = 'Y'  THEN
          IF l_glad0262 <> '1' THEN
             IF cl_null(g_glam.glam069) THEN
                LET r_success = FALSE
                INITIALIZE g_errparam TO NULL
                LET g_errparam.extend = l_msg1
                LET g_errparam.code   = "agl-00083"
                LET g_errparam.popup  = TRUE 
                CALL cl_err()
             END IF  
          END IF
          IF NOT cl_null(g_glam.glam069) THEN        
             #作业编号aglt3[789]0且核算项值为*时不检查
             IF g_prog MATCHES 'aglt3[789]0' AND g_glam.glam069 = '*' THEN  
             ELSE
                CALL s_voucher_free_account_chk(l_glad0261,g_glam.glam069,l_glad0262) RETURNING l_errno
                IF NOT cl_null(l_errno) THEN
                   LET r_success = FALSE
                   LET l_errmsg = l_msg||'/'||g_glam.glam069
                   INITIALIZE g_errparam TO NULL
                   LET g_errparam.extend = l_errmsg
                   LET g_errparam.code   = l_errno
                   IF l_errno = 'sub-01302' THEN
                      LET g_errparam.replace[1] = 'agli041'
                      LET g_errparam.replace[2] = cl_get_progname('agli041',g_lang,"2")
                      LET g_errparam.exeprog = 'agli041'
                   END IF
                   LET g_errparam.popup  = TRUE 
                   CALL cl_err()
                END IF 
             END IF
          END IF 
       ELSE
          IF g_glam.glam069 <> ' ' OR g_glam.glam069 IS NULL THEN  
             LET r_success = FALSE
             INITIALIZE g_errparam TO NULL
             LET g_errparam.extend = l_msg1
             LET g_errparam.code   = "agl-00357"
             LET g_errparam.popup  = TRUE 
             CALL cl_err()
          END IF
       END IF
       #180920-00003#1 add -e
   END FOREACH
   
   #151103-00017#1--add--str--
   #(8)当为aglt390时，检查glan_t金额分摊来源档中维护的资料
#   IF g_prog='aglt390' THEN   #170301-00030#8 by 09257 --mark
   IF g_prog MATCHES 'aglt390*' THEN   #170301-00030#8 by 09257 --add  
      CALL s_voucher_glan_chk(p_glalld,p_glaldocno,l_glaldocdt) RETURNING l_success
      IF l_success = FALSE THEN
         LET r_success = FALSE
      END IF
   END IF
   #151103-00017#1--add--end
   
   RETURN r_success
# 常用传票单身科目检查
END FUNCTION


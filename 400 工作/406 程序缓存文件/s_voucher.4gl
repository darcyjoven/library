#            CALL cl_err()
         END IF
      END IF
   END IF
  
   #該科目做利潤成本管理時，利潤成本不可空白  
   IF l_glad008 = 'Y' THEN
      IF cl_null(g_glam.glam021) THEN
         LET r_success = FALSE

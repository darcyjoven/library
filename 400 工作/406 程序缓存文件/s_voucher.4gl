#                  RETURNING r_success
# Input parameter: p_ld           帐套
#                : p_wc           QBE条件(帐款来源)
#                : p_sum_type     汇总方式
#                : p_source       單據來源
# Return code....: r_success      成功否标识位
# Date & Author..: 2014/07/04 By lujh
# Modify.........: #201230-00060#4 add p_source
################################################################################
PUBLIC FUNCTION s_voucher_gen_nm_1_def_cursor(p_ld,p_wc,p_sum_type,p_date,p_source)
   DEFINE p_ld            LIKE glaa_t.glaald
   DEFINE p_wc            LIKE type_t.chr1000
   DEFINE p_sum_type      LIKE type_t.chr1
   DEFINE p_source        LIKE type_t.chr10 #201230-00060#4 add
   DEFINE r_success       LIKE type_t.num5
   #DEFINE l_sql           LIKE type_t.chr1000  #201207-00021#2 mark
   DEFINE l_sql           STRING #201207-00021#2 add
   DEFINE l_glaa004       LIKE glaa_t.glaa004
   DEFINE l_field         LIKE type_t.chr50
   DEFINE p_date          LIKE type_t.dat
   DEFINE l_glab005d      LIKE glab_t.glab005      #借方科目
   DEFINE l_glab005c      LIKE glab_t.glab005      #贷方科目


   WHENEVER ERROR CONTINUE
   LET r_success = FALSE 

   #201230-00060#4 add ---s
   LET l_glaa004 = NULL
   SELECT glaa004 INTO l_glaa004
     FROM glaa_t
    WHERE glaaent = g_enterprise
      AND glaald = p_ld
      AND glaa014 = 'Y'
   #201230-00060#4 add ---e

   #190411-00045#15 add -s
   IF g_prog MATCHES 'anmt610*' THEN
      LET l_sql = "SELECT nmbaent,nmbaownid,nmbaowndp,nmbacrtid,nmbacrtdp,nmbacrtdt,nmbamodid,",
                  "       nmbamoddt,nmbacnfid,nmbacnfdt,nmbastus,nmbacomp,nmbadocno,nmbadocdt,",
                  "       nmbasite,nmba001,nmba002,nmba003,nmba004,nmba005,nmba006,",
                  "       nmba007,nmba008,nmba009,nmba010,nmba011,nmbaud001,nmbaud002,",
                  "       nmbaud003,nmbaud004,nmbaud005,nmbaud006,nmbaud007,nmbaud008,nmbaud009,",
                  "       nmbaud010,nmbaud011,nmbaud012,nmbaud013,nmbaud014,nmbaud015,nmbaud016,",
                  "       nmbaud017,nmbaud018,nmbaud019,nmbaud020,nmbaud021,nmbaud022,nmbaud023,",
                  "       nmbaud024,nmbaud025,nmbaud026,nmbaud027,nmbaud028,nmbaud029,nmbaud030,",
                  "       nmba012,nmba013,nmba014,nmba015,nmba016",
                  "  FROM nmba_t,glaa_t ",
                  " WHERE nmbaent = ",g_enterprise,  
                  "   AND nmbaent = glaaent AND nmbacomp = glaacomp AND glaald = '",p_ld CLIPPED,"'",
                  "   AND nmba016 IS NULL ",
                  "   AND ",p_wc

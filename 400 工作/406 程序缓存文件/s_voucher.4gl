#                       "       SUM(c),qty,SUM(sum),glaq039,SUM(glaq040),SUM(glaq041),",
#                       "       glaq042,SUM(glaq043),SUM(glaq044),'','','','','','','' ",
#                       "       ,CASE WHEN d>0 THEN 1 ELSE 2 END ",  
                       #180808-00051#3 mark -e
                       #180808-00051#3 add -s
                       "       glaq035,glaq036,glaq037,glaq038,glbc004,",l_select1,"'','','','','','','' ",
                       "       ",l_sw,       
                       #180808-00051#3 add -e
                       "  FROM s_vr_tmp05"
                       #180808-00051#3 mark -s
#                       " GROUP BY CASE WHEN d>0 THEN 1 ELSE 2 END,glaqent,glaqcomp,glaq001,glaq002,glaq005, glaq006,glaq007,glaq009,glaq011,",  
#                       "          glaq012,glaq013,glaq014,glaq015,glaq016,  glaq017,glaq018,glaq019,glaq020,glaq021,",
#                       "          glaq022,glaq023,glaq024,glaq025,glaq027,glaq028,glaq051,glaq052,glaq053,",
#                       "          glaq029,glaq030,glaq031,glaq032,glaq033,  glaq034,glaq035,glaq036,glaq037,glaq038,glbc004,qty,",
#                       "          glaq039,glaq042" 
                       #180808-00051#3 mark -e
        #180808-00051#3 add -s
        IF p_acctype = '1' THEN
           LET l_sql = l_sql,"  ",l_group
        ELSE
           #PGS(D)-10500 -S-
          #LET l_sql = l_sql," GROUP BY ",l_group,"glaqent,glaqcomp,glaq001,glaq002,glaq005, glaq006,glaq007,glaq009,glaq011,",
           LET l_sql = l_sql," GROUP BY ",l_group,"glaqent,glaqcomp,glaq001,glaq002,glaq005, glaq006,glaq007,glaq009,glaq011,",
           #PGS(D)-10500 -E-
                             "          glaq012,glaq013,glaq014,glaq015,glaq016,  glaq017,glaq018,glaq019,glaq020,glaq021,",                             "          glaq022,glaq023,glaq024,glaq025,glaq027,glaq028,glaq051,glaq052,glaq053,",
                             "          glaq029,glaq030,glaq031,glaq032,glaq033,  glaq034,glaq035,glaq036,glaq037,glaq038,glbc004,qty,",
                             "          glaq039,glaq042" 
        END IF
        #180808-00051#3 add -e
        END IF           
        PREPARE s_voucher_tmp_p FROM l_sql
        DECLARE s_voucher_tmp_c CURSOR FOR s_voucher_tmp_p

        FOREACH s_voucher_tmp_c INTO l_tmp.* 

            IF l_tmp.d = 0 AND l_tmp.c = 0 THEN
               CONTINUE FOREACH
            END IF
 
            
            #180808-00051#3 add -s 借貸合併後方向判斷:若acctype=2時且有紅字傳票&設定依餘額類型呈現->抓glac008
            IF p_acctype = '2' AND g_red_pn = 'Y' THEN
               LET l_glad002 = 'N' 
               LET l_glac008 = '1'
               EXECUTE s_voucher_p1 USING l_tmp.glaq002
                  INTO l_glad002,l_glac008
               
               #180808-00051#4 add -s 
               #若来源类型为冲账且會科存在於沖銷類型為(11~14)單據中才做紅字處理
               #IF l_glad002 = 'Y' AND p_type_sour = '2' THEN #190507-00025#2 mark
               IF l_glad002 = 'Y' AND (p_type_sour = '2' OR l_type2 = '2') THEN #190507-00025#2 add
                  LET l_cnt = 0
                  EXECUTE s_voucher_p2 USING l_tmp.glaq002
                     INTO l_cnt
                  IF l_cnt <= 0 THEN
                     LET l_glad002 = 'N' 
                  END IF
               END IF
               #180808-00051#4 add -e
               IF l_glad002 = 'Y' THEN #須按餘額類型呈現借貸
                  IF l_glac008 = '1' THEN #借方
                     LET l_tmp.d = l_tmp.d - l_tmp.c 
                     LET l_tmp.c = 0
                     LET l_tmp.glaq040 = l_tmp.glaq040 - l_tmp.glaq041
                     LET l_tmp.glaq041 = 0
                     LET l_tmp.glaq043 = l_tmp.glaq043 - l_tmp.glaq044
                     LET l_tmp.glaq044 = 0
                  ELSE                    #貸方
                     LET l_tmp.c = l_tmp.c - l_tmp.d 
                     LET l_tmp.d = 0
                     LET l_tmp.glaq041 = l_tmp.glaq041 - l_tmp.glaq040
                     LET l_tmp.glaq040 = 0
                     LET l_tmp.glaq044 = l_tmp.glaq044 - l_tmp.glaq043
                     LET l_tmp.glaq043 = 0
                  END IF
               END IF
            END IF
            #180808-00051#3 add -e 
            
            #借貸都有金額則拆兩筆
            IF l_tmp.d != 0 AND l_tmp.c != 0 THEN
               LET l_c = l_tmp.c
               LET l_glaq041 = l_tmp.glaq041
               LET l_glaq044 = l_tmp.glaq044
               
               #借   
               LET l_tmp.c = 0
               LET l_tmp.glaq041 = 0
               LET l_tmp.glaq044 = 0
               LET l_tmp.sw = 1
               LET l_tmp.sum = l_tmp.d
               INSERT INTO s_vr_tmp051 VALUES(l_tmp.*)  
               #貸 
               LET l_tmp.d = 0
               LET l_tmp.glaq040 = 0
               LET l_tmp.glaq043 = 0
               LET l_tmp.c = l_c
               LET l_tmp.glaq041 = l_glaq041
               LET l_tmp.glaq044 = l_glaq044
               LET l_tmp.sw = 2
               LET l_tmp.sum = l_tmp.c
               INSERT INTO s_vr_tmp051 VALUES(l_tmp.*)              
            ELSE
               IF l_tmp.d = 0 THEN
                  LET l_tmp.sw = 2
               ELSE
                  LET l_tmp.sw = 1
               END IF
               INSERT INTO s_vr_tmp051 VALUES(l_tmp.*)  
            END IF
        END FOREACH   
END FUNCTION

################################################################################
# Descriptions...: 检核科目是否满足agli060限制条件
# Memo...........: #170421-00027#8 add
# Usage..........: CALL s_voucher_hsx_glbi_chk(ls_js)
#                  RETURNING r_success
# Input parameter ls_js
#                 :glbi001    #作业编号 
#                 :glbild     #账套
#                 :docno      ##单号OR单别 
#                 :glbi003    #科目编号  
# Return code....: r_success      检查结果TRUE/FALSE
# Date & Author..: 2017/11/17 By 07900
# Modify.........:
################################################################################
PUBLIC FUNCTION s_voucher_hsx_glbi_chk(ls_js)
DEFINE ls_js        STRING
DEFINE lc_param  RECORD
       glbi001   LIKE glbi_t.glbi001,   #作业编号 
       glbild    LIKE glbi_t.glbild,    #账套
       docno     LIKE xrca_t.xrcadocno, #单号OR单别
       glbi003   LIKE glbi_t.glbi003    #科目编号  
             END RECORD
DEFINE r_success   LIKE type_t.num5
DEFINE l_slip      LIKE glbi_t.glbi004 
DEFINE l_glbi002   LIKE glbi_t.glbi002 
DEFINE l_cnt       LIKE type_t.num5 

   #核算项开窗检核增加agli061相关联判断，当agli061中有维护单别资料时，
   #如果是正向(允许)，科目只可以存在agli061中维护的单别+作业下；如果是反向(拒绝)，科目则不可以在agli061中单别+作业下；
   #如果没有维护agli061中的值，就按照原逻辑  
   CALL util.JSON.parse(ls_js,lc_param)
   
   CALL s_aooi200_fin_get_slip(lc_param.docno) RETURNING r_success,l_slip
   
   LET r_success=TRUE
  
   LET g_errno=''
   LET l_cnt = 0
   #作业+单别是否在agli061里作科目设限。
   SELECT COUNT(1) INTO l_cnt
     FROM glbi_t
    WHERE glbient = g_enterprise AND glbild = lc_param.glbild
      AND glbi001 = lc_param.glbi001 
      AND (glbi004 = l_slip OR glbi004 = 'ALL') 
      AND glbistus = 'Y'
   IF cl_null(l_cnt) THEN LET l_cnt = 0 END IF      
   IF l_cnt> 0 THEN    
      LET l_cnt = 0
      #是否有允许录入的科目      
      SELECT COUNT(1) 
        INTO l_cnt
        FROM glbi_t
       WHERE glbient = g_enterprise AND glbild = lc_param.glbild
         AND glbi001 = lc_param.glbi001 
         AND (glbi004 = l_slip OR glbi004 = 'ALL') 
         AND glbistus = 'Y' AND glbi002 = '1'
      IF cl_null(l_cnt) THEN LET l_cnt = 0 END IF   
      LET l_glbi002 = ''
      SELECT DISTINCT glbi002 
        INTO l_glbi002 
        FROM glbi_t
       WHERE glbient = g_enterprise AND glbild = lc_param.glbild
         AND glbi001 = lc_param.glbi001 AND glbi003 = lc_param.glbi003 
         AND (glbi004 = l_slip OR glbi004 = 'ALL') 
         AND glbistus = 'Y'
         
      CASE 
         #WHEN l_glbi002 = '1' #科目允许录入,不用报错
         WHEN l_glbi002 = '2'  LET g_errno = 'agl-00805' #科目拒绝录入
         WHEN (SQLCA.SQLCODE = 100 AND l_cnt > 0 ) LET g_errno = 'agl-00804'  #作业+单别有设限科目允许录入范围，但该科目不在此范围内         
      END CASE       
   END IF

   IF NOT cl_null(g_errno) THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = g_errno
      LET g_errparam.extend = lc_param.glbi003 
      LET g_errparam.popup = TRUE
      LET g_errparam.replace[1] = lc_param.glbi003 
      LET g_errparam.replace[2] = lc_param.glbi001
      LET g_errparam.replace[3] = l_slip
      CALL cl_err()
      LET r_success=FALSE
   END IF
   RETURN r_success
   
END FUNCTION

################################################################################
# Descriptions...: 返回抓取agli061中维护的科目的sql语句
# Memo...........: #170421-00027#8 add 
# Usage..........: s_voucher_get_glbi_sql(ls_js)
#                  RETURNING r_sql
# Input parameter ls_js
#                : glbi001   #作业编号
#                : glbild    #账套
#                : docno     #单号or单据
#                : glbi003   #科目编号
# Return code....: r_sql
# Date & Author..: 20171117 By 07900
# Modify.........:
################################################################################
PUBLIC FUNCTION s_voucher_get_glbi_sql(ls_js)
DEFINE ls_js        STRING
DEFINE lc_param  RECORD
       glbi001   LIKE glbi_t.glbi001,   #作业编号 
       glbild    LIKE glbi_t.glbild,    #账套
       docno     LIKE xrca_t.xrcadocno, #单号OR单别
       glbi003   LIKE glbi_t.glbi003    #科目        
             END RECORD
DEFINE r_sql     STRING 
DEFINE l_sql1    STRING
DEFINE l_sql2    STRING
DEFINE l_cnt     LIKE type_t.num5
DEFINE l_cnt1    LIKE type_t.num5
DEFINE l_cnt2    LIKE type_t.num5
DEFINE l_success LIKE type_t.num5 
DEFINE l_slip    LIKE glbi_t.glbi004 



   LET r_sql = ' 1=1'
   CALL util.JSON.parse(ls_js,lc_param)
   
   CALL s_aooi200_fin_get_slip(lc_param.docno) RETURNING l_success,l_slip
   
   #(1)判断agli061中是否维护科目资料,如果维护,组合抓取核算项值的sql语句,如果没有维护,返回' 1=1'
   LET l_cnt=0 
   SELECT COUNT(1)
     INTO l_cnt    
     FROM glbi_t
    WHERE glbient = g_enterprise
      AND glbild = lc_param.glbild
      AND glbi001 = lc_param.glbi001
      AND (glbi004 = l_slip OR glbi004 = 'ALL')
      AND glbistus = 'Y' 
   IF cl_null(l_cnt) THEN  LET l_cnt = 0 END IF    
   IF l_cnt = 0 THEN
      RETURN r_sql
   END IF    
    
   
   #正向（允许）
   #agli061里是否存在作业的单别下的正向科目
   LET l_cnt1 = 0
   SELECT COUNT(1)
     INTO l_cnt1    
     FROM glbi_t
    WHERE glbient = g_enterprise
      AND glbild = lc_param.glbild
      AND glbi001 = lc_param.glbi001
      AND (glbi004 = l_slip OR glbi004 = 'ALL')
      AND glbistus = 'Y' 
      AND glbi002 = '1'
   IF cl_null(l_cnt1) THEN  LET l_cnt1 = 0 END IF 
   IF l_cnt1 > 0 THEN  
      LET l_sql1 = " glac002 IN (SELECT DISTINCT glbi003 FROM glbi_t ",
                   "              WHERE glbient = ",g_enterprise," AND glbild = '",lc_param.glbild,"'",
                   "                AND  glbi001 = '",lc_param.glbi001,"'  AND glbi002 = '1'",
                   "                AND  (glbi004 = '",l_slip,"' OR glbi004 = 'ALL') ",
                   "                AND glbistus = 'Y' )"
   ELSE
      LET l_sql1 = " 1=1 "   
   END IF
   #反向（拒绝）
   #agli061里是否存在作业的单别下的反向科目
   LET l_cnt2 = 0
   SELECT COUNT(1)
     INTO l_cnt2    
     FROM glbi_t
    WHERE glbient = g_enterprise
      AND glbild = lc_param.glbild
      AND glbi001 = lc_param.glbi001
      AND (glbi004 = l_slip OR glbi004 = 'ALL')
      AND glbistus = 'Y' 
      AND glbi002 = '2'
   IF cl_null(l_cnt2) THEN  LET l_cnt2 = 0 END IF
   IF l_cnt2 > 0 THEN     
     LET l_sql2 = "  glac002 NOT IN (SELECT DISTINCT glbi003 FROM glbi_t ",
                  "                  WHERE glbient = ",g_enterprise," AND glbild = '",lc_param.glbild,"'",
                  "                    AND  glbi001 = '",lc_param.glbi001,"'  AND glbi002 = '2'",
                  "                    AND  (glbi004 = '",l_slip,"' OR glbi004 = 'ALL') ",
                  "                    AND glbistus = 'Y' )"
   ELSE
     LET l_sql2 = " 1=1 "
   END IF    
   LET r_sql  = l_sql1," AND ",l_sql2
   RETURN r_sql 

END FUNCTION

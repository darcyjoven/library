PUBLIC FUNCTION s_voucher_gen_ar(p_type,p_ld,p_date,p_slip,p_sum_type,p_wc,p_preview)
   #DEFINE p_type          LIKE type_t.chr2 #190507-00025#2 mark
   DEFINE p_type          LIKE type_t.chr10 #190507-00025#2 add
   DEFINE p_ld            LIKE glaa_t.glaald
   DEFINE p_date          LIKE glap_t.glapdocdt
  #DEFINE p_slip          LIKE ooba_t.ooba002    #160525-00021#1 mark
   DEFINE p_slip          LIKE type_t.chr50      #160525-00021#1   
   DEFINE p_sum_type      LIKE type_t.chr1
   DEFINE p_wc            LIKE type_t.chr1000
#   DEFINE p_preview       LIKE type_t.chr1       #180808-00051#3 mark
   DEFINE p_preview       LIKE type_t.chr10       #180808-00051#3 add
   DEFINE r_success       LIKE type_t.num5
   DEFINE r_start_no      LIKE glap_t.glapdocno
   DEFINE r_end_no        LIKE glap_t.glapdocno
   DEFINE l_site          LIKE ooef_t.ooef001
   DEFINE l_success       LIKE type_t.num5
   #180410-00038#1 add-s
   DEFINE l_glaq001       LIKE glaq_t.glaq001
   DEFINE l_glaq002       LIKE glaq_t.glaq002
   DEFINE l_sum           LIKE glaq_t.glaq003
   DEFINE l_glaqseq       LIKE glaq_t.glaqseq
   DEFINE l_sw            LIKE type_t.num5
   DEFINE l_red           LIKE type_t.chr1
   #180410-00038#1 add-e
   #180808-00051#3 add -s
   DEFINE l_type1         LIKE type_t.chr10  
   DEFINE l_acctype       LIKE type_t.chr1      #會科合併原則
   DEFINE l_preview       LIKE type_t.chr1      #是否先預覽后再拋憑證
   DEFINE l_tok           base.Stringtokenizer
   #190507-00025#2 add s---
   DEFINE l_type          LIKE type_t.chr10 
   DEFINE l_type2         LIKE type_t.chr10 
   DEFINE l_wc1           STRING
   #190507-00025#2 add e---
   #180808-00051#3 add -e
   DEFINE l_groupglbc     LIKE type_t.chr1   #201207-00021#2 add
   
   WHENEVER ERROR CONTINUE
   
   LET r_success  = FALSE
   CALL g_glaq_d.clear()
   
   #180808-00051#3 add -s
   LET l_preview = ''
   LET l_acctype = ''
   LET l_tok = base.StringTokenizer.create(p_preview,"@")
   WHILE l_tok.hasMoreTokens()
      IF cl_null(l_preview) THEN
         LET l_preview = l_tok.nextToken()
      ELSE
         #LET l_acctype = l_tok.nextToken() #201207-00021#2 mark
         #201207-00021#2 add ---s
         IF cl_null(l_acctype) THEN
            LET l_acctype = l_tok.nextToken()
         ELSE
            LET l_groupglbc = l_tok.nextToken()
         END IF         
         #201207-00021#2 add ---e
      END IF
   END WHILE
   LET p_preview = l_preview
   IF cl_null(l_acctype) THEN LET l_acctype = '3' END IF #沒有給會科合併原則的一律給3
   IF cl_null(l_groupglbc) THEN LET l_groupglbc = 'Y' END IF #201207-00021#2 add
   #180808-00051#3 add -e
   #190507-00025#2 add s---
   #l_type='1' l_type2='2'
   LET l_type = '' LET l_type2 = ''  
   LET l_tok = base.StringTokenizer.create(p_type,"@")
   WHILE l_tok.hasMoreTokens()
      IF cl_null(l_type) THEN
         LET l_type = l_tok.nextToken()
      ELSE
         IF cl_null(l_type2) THEN
            LET l_type2 = l_tok.nextToken()
         END IF  
      END IF
   END WHILE
   LET p_type = l_type   
   #190507-00025#2 add e---     

   #检查事务中
   IF NOT s_transaction_chk('Y',1) THEN
      RETURN r_success,r_start_no,r_end_no
   END IF   

   
   IF cl_null(p_ld) THEN
      #帐套参数为空
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'agl-00121'
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      CALL cl_err()

      RETURN r_success,r_start_no,r_end_no
   END IF
   
   #检查帐套是否正确
#   IF g_prog <> 'axrp141' AND g_prog <> 'axrp142' AND g_prog <> 'aapp141' AND g_prog <> 'aapp142' THEN    #170110-00009#1 add lujh   #170301-00030#8 by 09257 --mark
   IF g_prog NOT MATCHES 'axrp141*' AND g_prog NOT MATCHES 'axrp142*' AND g_prog NOT MATCHES 'aapp141*' AND g_prog NOT MATCHES 'aapp142*' THEN    #170110-00009#1 add lujh    #170301-00030#8 by 09257 --add  
      CALL s_ld_chk_authorization(g_user,p_ld) RETURNING l_success
      IF l_success = FALSE THEN
         INITIALIZE g_errparam TO NULL
         LET g_errparam.code = 'axr-00022'
         LET g_errparam.extend = p_ld
         LET g_errparam.popup = TRUE
         CALL cl_err()
      
         RETURN r_success,r_start_no,r_end_no
      END IF 
   END IF     #170110-00009#1 add lujh
   
   #检查单别CALL s_aooi200_chk_slip()
   #目前无法取得单别参数表号,需要再确认
   SELECT glaacomp INTO l_site FROM glaa_t
    WHERE glaaent = g_enterprise
      AND glaald  = p_ld
   IF SQLCA.sqlcode THEN
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = SQLCA.sqlcode
      LET g_errparam.extend = 'sel glaacomp'
      LET g_errparam.popup = TRUE
      CALL cl_err()

      RETURN r_success,r_start_no,r_end_no 
   END IF
#   CALL s_aooi200_chk_slip(l_site,'',p_slip,'aglt310')
#        RETURNING l_success
#   IF NOT l_success THEN
#      RETURN r_success,r_start_no,r_end_no    
#   END IF
   
   IF cl_null(p_sum_type) THEN
      #产生凭证的汇总方式为空
      INITIALIZE g_errparam TO NULL
      LET g_errparam.code = 'agl-00123'
      LET g_errparam.extend = ''
      LET g_errparam.popup = TRUE
      CALL cl_err()

      RETURN r_success,r_start_no,r_end_no 
   END IF
   
   IF cl_null(p_wc) THEN LET p_wc = " 1=1" END IF
   
   #CALL s_voucher_cre_ar_tmp_table() RETURNING r_success
   
   #190507-00025#2 add s--- 
   LET g_flag = '1'
   IF NOT cl_null(l_type2) THEN 
      LET g_flag = '2'    
   END IF
   #190507-00025#2 add e---
   
   CASE p_type
        WHEN '1'   #AR-应收立帐自动产生凭证
#                   CALL s_voucher_gen_ar_2(p_ld,p_sum_type,p_wc)  #180808-00051#3 mark
                   CALL s_voucher_gen_ar_2(p_ld,p_sum_type,p_wc,l_acctype)  #180808-00051#3 add  
                        RETURNING r_success
                        
                   #180410-00038#1 add-s #问题2：税额科目净额产生
                   #取最大金额
                   #180525-00030#1 mark-s
#                   SELECT CASE WHEN sum(d)-sum(c)>0 THEN max(d) ELSE max(c) END INTO l_sum    
#                     FROM s_vr_tmp04     
#                    WHERE flag='2'                  
#                   #根据最大金额取对应资料
#                   SELECT glaq001,glaq002,sw,glaqseq,red INTO l_glaq001,l_glaq002,l_sw,l_glaqseq,l_red
#                     FROM s_vr_tmp04     
#                    WHERE flag='2' AND (d=l_sum OR c=l_sum)
#                   #更新明细资料（汇总时自动将税额按净额产生）                    
#                   UPDATE s_vr_tmp04 SET (glaq001,glaq002,sw,glaqseq,red)=(l_glaq001,l_glaq002,l_sw,l_glaqseq,l_red)
#                    WHERE flag='2'
#                   IF SQLCA.sqlcode THEN
#                     INITIALIZE g_errparam TO NULL
#                     LET g_errparam.code = SQLCA.sqlcode
#                     LET g_errparam.extend = 'upd s_vr_tmp04'
#                     LET g_errparam.popup = TRUE
#                     CALL cl_err()
#                   END IF 
                   #180525-00030#1 mark-s                   
                   #180410-00038#1 add-e
                   
        WHEN '2'   #AR-应收冲帐自动产生凭证
#                   CALL s_voucher_gen_ar_3(p_ld,p_sum_type,p_wc)  #180808-00051#4 mark
                   CALL s_voucher_gen_ar_3(p_ld,p_sum_type,p_wc,l_acctype)  #180808-00051#4 add
                        RETURNING r_success
        WHEN '3'   #AR-重评价自动产生凭证
                   CALL s_voucher_gen_ar_4(p_ld,p_sum_type,p_wc,p_date)
                        RETURNING r_success
   END CASE
   
   #190507-00025#2 add s---
   IF NOT cl_null(l_type2) THEN 
      IF l_type2 = '2' THEN 
         LET l_wc1 = " xrdadocno IN (SELECT xrcadocno FROM axrp330_tmp WHERE sel = 'Y')"
         CALL s_voucher_gen_ar_3(p_ld,p_sum_type,l_wc1,l_acctype)  
                        RETURNING r_success
      END IF
   END IF
   #190507-00025#2 add e---
   
   #將明細資料彙總
#   CALL s_voucher_gen_ar_2_ins_group_tmp(p_type,p_ld,p_sum_type) RETURNING l_success  #180808-00051#3 mark 
   #180808-00051#3 add -s
   #LET l_type1 = p_sum_type,'@',l_acctype #原本參數p_sum_type用@分隔加掛acctype變數 #201207-00021#2 mark
   LET l_type1 = p_sum_type,'@',l_acctype,'@',l_groupglbc #201207-00021#2 add

         
   CALL s_voucher_gen_ar_2_ins_group_tmp(p_type,p_ld,l_type1) RETURNING l_success
   #180808-00051#3 add -e
   IF NOT l_success THEN
      RETURN r_success,r_start_no,r_end_no    
   END IF
   
   #190507-00025#2 add s---
   IF NOT cl_null(l_type2) THEN
      CALL s_voucher_gen_ar_2_ins_group_tmp(l_type2,p_ld,l_type1) RETURNING l_success
      IF NOT l_success THEN
         RETURN r_success,r_start_no,r_end_no    
      END IF
   END IF
   #190507-00025#2 add e---
   
   IF p_preview = 'Y' THEN   #先預覽,不產生憑證
      RETURN r_success,r_start_no,r_end_no
   ELSE                      #不預覽,直接產生憑證
      #因为交易客商前面无条件显示,抛凭证时才根据设置清空,这边做清空处理
      CALL s_voucher_gen_ar_2_glaq021_upd(p_ld) RETURNING l_success
      IF NOT l_success THEN
         RETURN r_success,r_start_no,r_end_no    
      END IF
      #IF p_type = '1' THEN  #170509-00094#12 add  #180129-00027#2 mark
      #IF p_type = '1' OR p_type = '2'THEN  #180129-00027#2 add #190507-00025#2 mark
      IF p_type = '1' OR p_type = '2' OR l_type2 = '2' THEN  #190507-00025#2 add
#         CALL s_voucher_ins_tmp(p_sum_type) #170509-00094#12 add #180808-00051#3 mark
          #190507-00025#2 add s---
          LET p_type = l_type
          IF NOT cl_null(l_type2) THEN
             LET p_type = p_type,"@",l_type2
          END IF
          #190507-00025#2 add e---
         CALL s_voucher_ins_tmp(p_sum_type,l_acctype,p_ld,p_type) #180808-00051#3 add
      END IF  #170509-00094#12 add  
      
      #190507-00025#2 add s---
      LET p_type = l_type
      IF NOT cl_null(l_type2) THEN
         LET p_type = p_type,"@",l_type2
      END IF
      #190507-00025#2 add e---
      
      #插入凭证单头
#      CALL s_voucher_gen_ar_2_ins_glap(p_type,p_slip,p_date,p_ld,p_sum_type)  #180808-00051#3 mark
      CALL s_voucher_gen_ar_2_ins_glap(p_type,p_slip,p_date,p_ld,l_type1)  #180808-00051#3 add
         RETURNING r_success,r_start_no,r_end_no
      IF NOT r_success THEN
         RETURN r_success,r_start_no,r_end_no       
      END IF
      RETURN r_success,r_start_no,r_end_no      
   END IF

################################################################################
END FUNCTION

PUBLIC FUNCTION s_voucher_forzen_glam005_chk(p_glalld,p_glaldocno,p_glam005,p_glaldocdt,p_success)
   DEFINE l_glad007        LIKE glad_t.glad007        #部門管理否
   DEFINE l_glad008        LIKE glad_t.glad008        #利潤成本中心管理
   DEFINE l_glad009        LIKE glad_t.glad009        #區域管理
   DEFINE l_glad010        LIKE glad_t.glad010        #客商管理

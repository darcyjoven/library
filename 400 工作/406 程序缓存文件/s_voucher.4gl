   #190528-00007#1 add ---s
   #CALL s_pre_voucher_chk_glac016(p_glaqld,p_glapdocno) RETURNING l_flag
   ##IF l_sfin0003 = '2' AND l_flag = 'Y' THEN #190619-00013#1 mark 
   #IF l_flag = 'Y' THEN #190619-00013#1 add 
   #   INITIALIZE l_glaq.* TO NULL
   #   INITIALIZE l_tmp.* TO NULL      
   #   IF p_acctype MATCHES '[23]' THEN 
   #     FOREACH s_voucher_sel_glaq_c USING p_glaqld,p_glapdocno INTO l_tmp.glaqseq,l_tmp.glaq002,l_tmp.sw #傳票
   #        FOREACH s_voucher_sel_glgb_c USING p_glapdocno,l_tmp.glaq002 INTO l_glaq.*,l_sw,l_glgb055 #底稿
   #190806-00013#1 mark ---e
   #190806-00013#1 add ---s
   CALL s_pre_voucher_chk_glac016(p_glaqld,p_glapdocno) RETURNING l_flag
   IF l_sfin0003 = '2' AND l_flag = 'Y' THEN
      INITIALIZE l_glaq.* TO NULL
      INITIALIZE l_tmp.* TO NULL      
      IF p_acctype MATCHES '[23]' THEN 
        FOREACH s_voucher_sel_glaq_c USING p_glaqld,p_glapdocno INTO l_tmp.* #傳票
           EXECUTE s_voucher_gen_ar_2_cs6 USING l_tmp.glaq002 INTO l_glad_r.* #201207-00021#2 add
           FOREACH s_voucher_sel_glgb_c  #底稿
             USING p_glapdocno,l_tmp.glaq001,l_tmp.glaq002,l_tmp.glaq005,l_tmp.glaq006,
                   l_tmp.glaq039,l_tmp.glaq042,l_tmp.glaq018,l_tmp.glaq019,l_tmp.glaq020,      
                   l_tmp.glaq023,l_tmp.glaq024,l_tmp.glaq025,l_tmp.glaq027,l_tmp.glaq028,       
                   l_tmp.glaq051,l_tmp.glaq052,l_tmp.glaq053,l_tmp.glaq029,l_tmp.glaq030,       
                   l_tmp.glaq031,l_tmp.glaq032,l_tmp.glaq033,l_tmp.glaq034,l_tmp.glaq035,       
                   l_tmp.glaq036,l_tmp.glaq037,l_tmp.glaq038,l_tmp.glaq007
                  ,l_tmp.glaq021,l_glad_r.glad010,l_tmp.glaq022,l_glad_r.glad027  #201207-00021#2 add
              INTO l_glaq.*,l_sw,l_glgb055 
   #190806-00013#1 add ---e                           
                                                      
              IF p_acctype = '3' AND l_tmp.sw <> l_sw THEN #借貸相反
                 CONTINUE FOREACH
              END IF               
              CALL s_voucher_ins_glac(p_glaqld,p_glapdocdt,p_glapdocno,l_tmp.glaqseq,l_tmp.glaq002,l_glgb055,l_glaq.glaq022,l_glaq.*) RETURNING l_success
              IF l_success = FALSE THEN 
                 RETURN r_success
              END IF
              INITIALIZE l_glaq.* TO NULL
           END FOREACH
        END FOREACH
      ELSE
         LET l_tmp.glaqseq = 0
         FOREACH s_voucher_sel_glgb_c USING p_glapdocno INTO l_glaq.*,l_sw,l_glgb055
            LET l_tmp.glaqseq = l_tmp.glaqseq + 1
            CALL s_voucher_ins_glac(p_glaqld,p_glapdocdt,p_glapdocno,l_tmp.glaqseq,l_glaq.glaq002,l_glgb055,l_glaq.glaq022,l_glaq.*) RETURNING l_success
            IF l_success = FALSE THEN 
               RETURN r_success
            END IF
            INITIALIZE l_glaq.* TO NULL
         END FOREACH      
      END IF           
   END IF
   #190528-00007#1 add ---e
      
   #一笔凭证处理完毕后,对凭证单头进行更行
   #总借/贷
   SELECT SUM(glaq003),SUM(glaq004) INTO l_glap010,l_glap011
     FROM glaq_t
    WHERE glaqent   = g_enterprise
      AND glaqld    = p_glaqld
      AND glaqdocno = p_glapdocno

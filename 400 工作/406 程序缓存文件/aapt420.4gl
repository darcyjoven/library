      THEN
         LET g_sql = "SELECT  DISTINCT apdeseq,apdeorga,apde002,apde006,apde003,apde008,apde021,apde024,apde100,apde109,apde101,apde119,apde032,apde013,apde014,apde015,apde016,apde010,apde009,apde039,apde040,apde041,apde011,apde012,apde046,apdecomp,apdesite,apde001,apdeseq,apde038,apde017,apde018,apde019, 

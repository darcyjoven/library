   DEFINE l_success   LIKE type_t.chr1  #151130-00015#2
   DEFINE l_slip      LIKE type_t.chr80  #151130-00015#2 
   DEFINE l_fields    STRING             #180508-00008#1 add
   DEFINE l_cnt       LIKE type_t.num5  #180706-00039#5 add
   DEFINE l_pmaa004   LIKE pmaa_t.pmaa004  #180926-00034#4 add
   #end add-point     
   
   #add-point:Function前置處理  name="set_no_entry.pre_function"
    CALL cl_set_comp_entry("apda015",FALSE) #151105-00007#2 add
    CALL cl_set_comp_required("apda015",FALSE) #180223-00024#1 add
   #end add-point
   
   IF p_cmd = 'u' AND g_chkey = 'N' THEN
      CALL cl_set_comp_entry("apdald,apdadocno",FALSE)
      #根據azzi850使用者身分開關特定欄位
      IF NOT cl_null(g_no_entry) THEN
         CALL cl_set_comp_entry(g_no_entry,FALSE)
      END IF
      #add-point:set_no_entry段欄位控制 name="set_no_entry.field_control"
     #CALL cl_set_comp_entry("apdasite,apda001,apda005",FALSE)  #150203  #180706-00039#5 mark
      CALL cl_set_comp_entry("apdasite,apda001",FALSE)  #180706-00039#5 add
      #end add-point 
   END IF 
   
   IF p_cmd = 'u' THEN  #docno,ld欄位確認是絕對關閉
      CALL cl_set_comp_entry("apdadocno,apdald",FALSE)
   END IF 
 
#  IF p_cmd = 'u' THEN  #docdt欄位依照設定關閉(FALSE則為設定不同意修正) #(ver:78)
      IF NOT cl_chk_update_docdt() THEN
         CALL cl_set_comp_entry("apdadocdt",FALSE)
      END IF
#  END IF 
   
   #add-point:set_no_entry段欄位控制後 name="set_no_entry.after_control"

   #151130-00015#2  -add -str
   IF NOT cl_null(g_apda_m.apdadocno) THEN  
      #获取单别
      CALL s_aooi200_fin_get_slip(g_apda_m.apdadocno) RETURNING l_success,l_slip
      #取得單別設置的"是否直接審核"參數
      CALL s_fin_get_doc_para(g_apda_m.apdald,g_apda_m.apdacomp,l_slip,'D-FIN-0033') RETURNING l_dfin0033
#      IF l_dfin0033 = "Y"  THEN     #190723-00011#1 mark
      IF (l_dfin0033 = "Y") OR (p_cmd = 'a') THEN    #190723-00011#1 add (p_cmd = 'a') 新增時不受D-FIN-0033限制
         CALL cl_set_comp_entry("apdadocdt",TRUE)
      #190723-00011#1 add --s
      ELSE
         CALL cl_set_comp_entry("apdadocdt",FALSE)
      #190723-00011#1 add --e
      END IF
      #180508-00008#1 add(s)
      LET l_fields = ''
      CALL s_aooi200_fin_get_doc_fields(g_site,'2',g_apda_m.apdadocno,g_apda_m.apdald) RETURNING l_fields
      CALL cl_set_comp_entry(l_fields,FALSE)
      #180508-00008#1 add(e) 
      #180706-00039#5---add---(S)
      LET l_cnt = 0
      SELECT COUNT(1) INTO l_cnt
        FROM apce_t
        LEFT JOIN apda_t ON apdaent = apceent AND apdald = apceld AND apdadocno = apcedocno
       WHERE apceent = g_enterprise
         AND apcedocno = g_apda_m.apdadocno
         AND apce003 IS NOT NULL
      IF l_cnt = 0 THEN
         SELECT COUNT(1) INTO l_cnt
           FROM apde_t
           LEFT JOIN apda_t ON apdaent = apdeent AND apdald = apdeld AND apdadocno = apdedocno
          WHERE apdeent = g_enterprise
            AND apdedocno = g_apda_m.apdadocno
            AND apde003 IS NOT NULL
      END IF
      IF l_cnt > 0 THEN
         CALL cl_set_comp_entry("apda005",FALSE)
         #180926-00034#4 add -s
         IF g_prog = 'axrt401' THEN
            CALL cl_set_comp_entry("apda004",FALSE) 
         END IF
         #180926-00034#4 add -e
      END IF
      #180706-00039#5---add---(E)  
      #180926-00034#4 add -s
      IF g_prog = 'axrt401' THEN
         LET l_pmaa004 = ''
         SELECT pmaa004 INTO l_pmaa004 
           FROM pmaa_t 
          WHERE pmaaent = g_enterprise 
            AND pmaa001 = g_apda_m.apda005
         IF l_pmaa004 = '2' THEN
            CALL cl_set_comp_entry('apda004',FALSE)
         END IF  
      END IF
      #180926-00034#4 add -e         
   END IF 
   #151130-00015#2  -end -str
   #end add-point 
 
END FUNCTION
 

################################################################################
# Descriptions...: 轉成SQL
# Memo...........: #161014-00053#3
# Usage..........: CALL aapt420_get_ooef001_wc(p_wc)
#                  RETURNING 回传参数
# Input parameter: p_wc 帳套
# Return code....: r_wc ('帳套')
# Date & Author..: 161020 By 08171
# Modify.........:
################################################################################
PRIVATE FUNCTION aapt420_get_ooef001_wc(p_wc)
   DEFINE p_wc       STRING
   DEFINE r_wc       STRING
   DEFINE tok        base.StringTokenizer
   DEFINE l_str      STRING

   LET tok = base.StringTokenizer.create(p_wc,",")
   WHILE tok.hasMoreTokens()
      IF cl_null(l_str) THEN
         LET l_str = tok.nextToken()
      ELSE
         LET l_str = l_str,"','",tok.nextToken()
      END IF      
   END WHILE   
   LET r_wc = "('",l_str,"')"
   
   RETURN r_wc
END FUNCTION

################################################################################
# Descriptions...: 取得apce003單號帳款日期
# Memo...........:
# Usage..........: CALL aapt420_get_apcadocdt()
# Input parameter: 無
# Return code....: 無
# Date & Author..: 170511 By 08729
# Modify.........: 170502-00035#2 add
################################################################################
PRIVATE FUNCTION aapt420_get_apcadocdt()
   
   IF cl_null(g_apda_m.apdald) OR cl_null(g_apce_d[l_ac].apce003) THEN
      RETURN
   END IF
   CASE 
      #帳款類型4開頭為應付
      WHEN g_apce_d[l_ac].apce002[1,1] = '4'   
         SELECT apcadocdt INTO g_apce_d[l_ac].l_apcadocdt
           FROM apca_t
          WHERE apcaent = g_enterprise
            AND apcald = g_apda_m.apdald
            AND apcadocno = g_apce_d[l_ac].apce003
         DISPLAY BY NAME g_apce_d[l_ac].l_apcadocdt
      #帳款類型3開頭為應收   
      WHEN g_apce_d[l_ac].apce002[1,1] = '3'   
         SELECT xrcadocdt INTO g_apce_d[l_ac].l_apcadocdt
           FROM xrca_t
          WHERE xrcaent = g_enterprise
            AND xrcald = g_apda_m.apdald
            AND xrcadocno = g_apce_d[l_ac].apce003
         DISPLAY BY NAME g_apce_d[l_ac].l_apcadocdt   
   END CASE   
END FUNCTION


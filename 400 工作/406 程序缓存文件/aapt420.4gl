               #160203-00009#1--(E)
            OTHERWISE
               LET la_param.prog = ''
         END CASE      
      
      WHEN 'apde014'
         IF cl_null(g_apce2_d[g_detail_idx].apde014) THEN
            INITIALIZE g_errparam TO NULL
            LET g_errparam.code   = 'sub-00280'
            LET g_errparam.extend = s_fin_get_colname(g_prog,'apde014')
            LET g_errparam.popup = TRUE
            CALL cl_err()

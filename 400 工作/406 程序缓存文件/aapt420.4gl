         END IF
         CASE g_apce2_d[g_detail_idx].apde002 
            #WHEN g_apce2_d[g_detail_idx].apde002 = '20' OR g_apce2_d[g_detail_idx].apde002 ='22' #180803-00064#1  mark
            WHEN  '20'                                                                            #180803-00064#1  add            

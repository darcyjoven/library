         #150901-00020#3--e
         #180921-00048#1---add---(S)
         EXECUTE aapt420_pmaa004_2 USING g_apce_d[l_ac].apce038 INTO l_pmaa004_b
         IF l_pmaa004_b = '2' THEN
            IF g_apce_d[l_ac].apce002 MATCHES '3*' THEN #應收單
               EXECUTE aapt420_xrca057_2 USING g_apce_d[l_ac].apce003 INTO l_apca057
            ELSE
               EXECUTE aapt420_apca057_2 USING g_apce_d[l_ac].apce003 INTO l_apca057
            END IF
            CALL s_axrt300_xrca_ref('xrca057',g_apce_d[l_ac].apce038,l_apca057,'') RETURNING g_sub_success,g_apce_d[l_ac].apce038_desc

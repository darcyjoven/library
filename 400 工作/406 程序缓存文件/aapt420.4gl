PRIVATE FUNCTION aapt420_get_apca057()
   IF cl_null(g_apda_m.apdald) OR cl_null(g_apce_d[l_ac].apce003) THEN
      RETURN
   END IF
   SELECT apca057 INTO g_apce_d[l_ac].l_apca057
